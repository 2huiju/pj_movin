{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/units/board/detail/BoardDetail_styles.ts",
    "src/components/units/board/detail/BoardDetail_queries.ts",
    "src/components/units/board/detail/BoardDetail_presenter.tsx",
    "src/components/units/board/detail/BoardDetail_container.tsx",
    "pages/boards/new/index.tsx",
    "pages/boards/index.tsx",
    "pages/boards/[Board]/index.tsx",
    "pages/boards/[Board]/edit/index.tsx",
    "pages/product/[Product]/index.tsx",
    "src/components/units/board/write/BoardWrite.styles.ts",
    "src/components/units/board/write/BoardWrite.queries.ts",
    "src/components/units/board/write/BoardWrite_presenter.tsx",
    "src/components/units/board/write/BoardWrite_container.tsx",
    "src/components/units/board/list/BoardList.styles.ts",
    "src/components/units/board/list/BoardList.queries.ts",
    "src/components/units/board/list/BoardList.presenter.tsx",
    "src/components/units/board/list/BoardList.container.tsx",
    "src/components/commons/todaySidebar/index.tsx",
    "src/components/commons/inquire/index.tsx"
  ],
  "exclude": ["node_modules"]
}
